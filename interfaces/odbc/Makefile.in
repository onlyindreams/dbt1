# Makefile
# 20 february 2002

include @TOPDIR@/make.common

CFLAGS_ODBC=$(CFLAGS) -DPHASE1

all: $(ODBC_OBJS)

%.so:%.c
	$(CC) -c -shared -fpic $(CFLAGS_ODBC) -o $@ $<

clean:
	rm -f $(ODBC_OBJS)

explain:
	@echo "$(ODBC_OBJS)"

DBOBJS: \
	Makefile \
	$(TOPDIR)/make.common \
	$(ODBC_INCLUDE) \
	$(ODBC_PRGS)
